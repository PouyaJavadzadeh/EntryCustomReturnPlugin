         </group>
         <group targetFramework="Xamarin.iOS10">
           <dependency id="Xamarin.Forms" version="2.3.5.256-pre6" />
         </group>
         <group targetFramework="portable-net45+win+wpa81+wp80">
           <dependency id="Xamarin.Forms" version="2.3.5.256-pre6" />
         </group>
         <group targetFramework="uap">
           <dependency id="Xamarin.Forms" version="2.3.5.256-pre6" />
         </group>
       </dependencies>
